{
	"semi": true,
	"tabWidth": 4,
	"useTabs": true,
	"singleQuote": true,
	"printWidth": 100,
	"trailingComma": "all",
	"overrides": [
		{
			"files": [
				"*.json5"
			],
			"options": {
				"singleQuote": false,
				"quoteProps": "preserve"
			}
		},
		{
			"files": [
				"*.yml"
			],
			"options": {
				"singleQuote": false
			}
		}
	]
}